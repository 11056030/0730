<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <stroke
        android:width="1dp"
        android:color="#ffffff" /> <!-- 定義框線的粗細和顏色 -->
    <corners android:radius="10dp" /> <!-- 設置圓角半徑 -->
    <padding
        android:left="1dp"
        android:top="1dp"
        android:right="1dp"
        android:bottom="1dp" /> <!-- 定義框線與內容的間距 -->
</shape>
